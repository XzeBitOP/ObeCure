      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/screenshot_phone.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Interactive mobile diet planner and workout timer"
    },
    {
      "src": "/screenshots/screenshot_desktop.jpg",
      "sizes": "1920x1080",
      "type": "image/jpeg",
      "form_factor": "wide",
      "label": "Desktop view of the progress and body composition charts"
    }
  ],
  "shortcuts": [
    {
      "name": "Create New Diet Plan",
      "short_name": "New Plan",
      "description": "Generate a new personalized diet plan.",
      "url": "/?view=planner",
      "icons": [{ "src": "/android/96.png", "sizes": "96x96", "type": "image/png" }]
    },
    {
      "name": "View Workouts",
      "short_name": "Workouts",
      "description": "Access your workout plans.",
      "url": "/?view=workouts",
      "icons": [{ "src": "/android/96.png", "sizes": "96x96", "type": "image/png" }]
    },
    {
      "name": "Log Sleep",
      "short_name": "Log Sleep",
      "description": "Log your sleep for the night.",
      "url": "/?action=logsleep",
      "icons": [{ "src": "/android/96.png", "sizes": "96x96", "type": "image/png" }]
    }
  ],
  "share_target": {
    "action": "/?action=share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/?action=handlefile",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "prefer_related_applications": false,
  "related_applications": [],
  "iarc_rating_id": "",
  "widgets": [],
  "edge_side_panel": {
    "preferred_width": 400
  },
